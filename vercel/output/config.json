{
    "version": 3,
    "routes": [
        {
            "src": "/bg.png",
            "dest": "/bg.png"
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
